	end
	
	function GuiLibrary:CreateDestroyButton(text,fontsize)
		local text = text or "Destroy Gui"
		local fontsize = fontsize or 27
		local TextButton = Instance.new("TextButton")	
		TextButton.Parent = Container
		TextButton.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
		TextButton.BorderSizePixel = 0
		TextButton.Position = UDim2.new(0.0492359921, 0, 0.0859106556, 0)
		TextButton.Size = UDim2.new(0, 121, 0, 25)
		TextButton.Font = Enum.Font.SourceSans
		TextButton.TextColor3 = Color3.fromRGB(255,255,255)
		TextButton.TextSize = fontsize
		TextButton.Text = text
		TextButton.ClipsDescendants = true
		TextButton.AutoButtonColor = false
		
		TextButton.MouseButton1Click:Connect(function()
			LucianUI:Destroy()
			if game.CoreGui:FindFirstChild("LucianUiNotification") then
				game.CoreGui.LucianUiNotification:Destroy()
			end
		end)
		
		TextButton.MouseEnter:Connect(function()
			game:GetService("TweenService"):Create(TextButton, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(25,25,25)}):Play()
		end)

		TextButton.MouseLeave:Connect(function()
			game:GetService("TweenService"):Create(TextButton, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(30,30,30)}):Play()
		end)
	end
	
	function GuiLibrary:CreateRadioButton(text, callback)
		local text = text or "New Radio Button"
		local callback = callback or function() end
		local RadioButtonLabel = Instance.new("Frame")
		local Label = Instance.new("TextLabel")
		local RadioContainer = Instance.new("Frame")
		local Outline = Instance.new("ImageLabel")
		local RadioButton = Instance.new("ImageButton")
		local Circle = Instance.new("ImageLabel")
		local radiobuttontoggle = false

		RadioButtonLabel.Name = "RadioButtonLabel"
		RadioButtonLabel.Parent = Container
		RadioButtonLabel.BackgroundColor3 = Color3.fromRGB(29, 29, 30)
		RadioButtonLabel.BackgroundTransparency = 1.000
		RadioButtonLabel.BorderColor3 = Color3.fromRGB(33, 95, 222)
		RadioButtonLabel.BorderSizePixel = 0
		RadioButtonLabel.Position = UDim2.new(0, 250, 0, 60)
		RadioButtonLabel.Size = UDim2.new(0, 350, 0, 24)

		Label.Name = "Label"
		Label.Parent = RadioButtonLabel
		Label.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Label.BackgroundTransparency = 1.000
		Label.ClipsDescendants = true
		Label.LayoutOrder = 1
		Label.Position = UDim2.new(0.25, 1, 0, 0)
		Label.Size = UDim2.new(1, -10, 1, 0)
		Label.Font = Enum.Font.Gotham
		Label.Text = text
		Label.TextColor3 = Color3.fromRGB(255, 255, 255)
		Label.TextSize = 14.000
		Label.TextXAlignment = Enum.TextXAlignment.Left

		RadioContainer.Name = "RadioContainer"
		RadioContainer.Parent = RadioButtonLabel
		RadioContainer.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		RadioContainer.BackgroundTransparency = 1.000
		RadioContainer.BorderColor3 = Color3.fromRGB(33, 95, 222)
		RadioContainer.Size = UDim2.new(0.0799999982, 0, 1, 0)

		Outline.Name = "Outline"
		Outline.Parent = RadioContainer
		Outline.AnchorPoint = Vector2.new(1, 1)
		Outline.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Outline.BackgroundTransparency = 1.000
		Outline.BorderColor3 = Color3.fromRGB(255, 255, 255)
		Outline.Position = UDim2.new(1, 0, 1, 0)
		Outline.Size = UDim2.new(1, 0, 1, 0)
		Outline.ZIndex = 0
		Outline.Image = "rbxassetid://4504304159"

		RadioButton.Name = "RadioButton"
		RadioButton.Parent = RadioContainer
		RadioButton.AnchorPoint = Vector2.new(0.5, 0.5)
		RadioButton.BackgroundColor3 = Color3.fromRGB(47, 47, 48)
		RadioButton.BackgroundTransparency = 1.000
		RadioButton.BorderColor3 = Color3.fromRGB(255, 255, 255)
		RadioButton.BorderSizePixel = 0
		RadioButton.Position = UDim2.new(0.6, 0, 0.5, 0)
		RadioButton.Selectable = false
		RadioButton.Size = UDim2.new(2, 9.2, 1, 4.2)
		RadioButton.AutoButtonColor = false
		RadioButton.Image = "rbxassetid://4504304159"
		RadioButton.ImageColor3 = Color3.fromRGB(47, 47, 48)
		RadioButton.ClipsDescendants = true

		Circle.Name = "Circle"
		Circle.Parent = RadioButton
		Circle.AnchorPoint = Vector2.new(0.5, 0.5)
		Circle.BackgroundColor3 = Color3.fromRGB(33, 95, 222)
		Circle.BackgroundTransparency = 1.000
		Circle.BorderSizePixel = 0
		Circle.Position = UDim2.new(0.5, 0, 0.5, 0)
		Circle.Size = UDim2.new(1, 1, 0.9, 3)
		Circle.Image = "rbxassetid://4504304159"
		Circle.ImageColor3 = Color3.fromRGB(33, 95, 222)
		Circle.Visible = false
		Circle.ClipsDescendants = true
		
		RadioButton.MouseButton1Click:Connect(function()
			if radiobuttontoggle == false then
				Circle.Visible = true
				radiobuttontoggle = true
				Ripple(Circle)
				pcall(callback, radiobuttontoggle)
			else
				Circle.Visible = false
				radiobuttontoggle = false
				Ripple(Circle)
				pcall(callback, radiobuttontoggle)
			end
		end)
	end
	function GuiLibrary:CreateSlider(text,max,callback)
		local text = text or "New Slider"
		local max = max or 100
		local callback = callback or function() end
		local Slider = Instance.new("Frame")
		local Background = Instance.new("Frame")
		local Bar = Instance.new("Frame")
		local SliderValue = Instance.new("TextLabel")
		local Title = Instance.new("TextLabel")
		local Dragger = Instance.new("ImageButton")

		Slider.Name = "Slider"
		Slider.Parent = Container
		Slider.BackgroundColor3 = Color3.fromRGB(29, 29, 30)
		Slider.BackgroundTransparency = 1.000
		Slider.BorderSizePixel = 0
		Slider.Position = UDim2.new(0, 500, 0, 200)
		Slider.Size = UDim2.new(0, 300, 0, 20)

		Background.Name = "Background"
		Background.Parent = Slider
		Background.AnchorPoint = Vector2.new(0.5, 0.5)
		Background.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Background.BackgroundTransparency = 1.000
		Background.Position = UDim2.new(0.5, 0, 0.3, 0)
		Background.Size = UDim2.new(1, 0, 1, 0)

		Bar.Name = "Bar"
		Bar.Parent = Background
		Bar.AnchorPoint = Vector2.new(0.5, 0.5)
		Bar.BackgroundColor3 = Color3.fromRGB(17, 154, 222)
		Bar.BorderSizePixel = 0
		Bar.LayoutOrder = 1
		Bar.Position = UDim2.new(0.5, 0, 0.9, 0)
		Bar.Size = UDim2.new(1, 0, 0.125, 0)

		SliderValue.Name = "SliderValue"
		SliderValue.Parent = Background
		SliderValue.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		SliderValue.BackgroundTransparency = 1.000
		SliderValue.BorderColor3 = Color3.fromRGB(27, 42, 53)
		SliderValue.BorderSizePixel = 0
		SliderValue.Position = UDim2.new(-0.203333333, 0, -0.5, 0)
		SliderValue.Size = UDim2.new(0, 59, 0, 48)
		SliderValue.Font = Enum.Font.SourceSans
		SliderValue.Text = "0"
		SliderValue.TextColor3 = Color3.fromRGB(255, 255, 255)
		SliderValue.TextSize = 23.000

		Title.Name = "Title"
		Title.Parent = Background
		Title.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Title.BackgroundTransparency = 1.000
		Title.BorderColor3 = Color3.fromRGB(27, 42, 53)
		Title.BorderSizePixel = 0
		Title.Position = UDim2.new(0.1, -15, 0, -11)
		Title.Size = UDim2.new(0, 173, 0, 40)
		Title.Font = Enum.Font.SourceSans
		Title.Text = text
		Title.TextColor3 = Color3.fromRGB(255, 255, 255)
		Title.TextSize = 21

		Dragger.Name = "Dragger"
		Dragger.Parent = Slider
		Dragger.AnchorPoint = Vector2.new(0.5, 0.5)
		Dragger.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Dragger.BackgroundTransparency = 1.000
		Dragger.Position = UDim2.new(0, 0, 0.7, 0)
		Dragger.Size = UDim2.new(0.5, 0, 0.5, 0)
		Dragger.SizeConstraint = Enum.SizeConstraint.RelativeYY
		Dragger.ZIndex = 4
		Dragger.Image = "rbxassetid://4504304159"
		Dragger.ImageColor3 = Color3.fromRGB(33, 95, 222)

		local UserInputService = game:GetService("UserInputService")
		local Dragging = false
		Dragger.MouseButton1Down:Connect(function()
			Dragging = true
		end)

		UserInputService.InputChanged:Connect(function()
			if Dragging then
				local MousePos = UserInputService:GetMouseLocation()+Vector2.new(0,36)
				local RelPos = MousePos-Bar.AbsolutePosition
				local Precent = math.clamp(RelPos.X/Bar.AbsoluteSize.X,0,1)
				Dragger:TweenPosition(UDim2.new(Precent,0,0.7,0),"InOut","Quad",0.01)
				local Real = Precent * max
				local Final = math.floor(Real)
				SliderValue.Text = Final
				pcall(callback, Final)
			end
		end)

		UserInputService.InputEnded:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				Dragging = false
			end
		end)
	end
	return GuiLibrary
end
return library
